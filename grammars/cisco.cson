  'ephone_dn':
    'patterns': [
      {
        'begin': '^(ephone-dn)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '^(!)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
        'patterns': [
          {
            'include': '#description'
          }
          {
            'match': '(dual-line|number|no-reg|label|name|call-forward|busy|noan|hold-alert|timeout|originator|secondary|mwi-type|mwi)\\s'
