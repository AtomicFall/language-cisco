    'patterns': [
      {
        'match': '\\b(no(?=\\s)|shutdown)'
        'name': 'keyword.negative.cisco'
      }
    ]
  'enable':
    'patterns': [
      {
        'begin': '^(enable)\\s(secret|password)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'keyword.other.enable'
        'end': '(.*$)'
        'endCaptures':
          '1':
            'name': 'entity.character.password.cisco'
      }
    ]
  'ephone':
    'patterns': [
      {
        'begin': '^(ephone)(?=\\s)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
        'end': '^(!)'
        'endCaptures':
          '1':
            'name': 'comment.documentation.cisco'
