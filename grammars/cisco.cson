  'banner':
    'patterns': [
      {
        'begin': '^(banner motd)(.*)'
        'beginCaptures':
          '1':
            'name': 'constant.language.cisco'
          '2':
            'name': 'comment.banner.cisco'
        'end': '^(\\^.*)'
        'endCaptures':
          '1':
            'name': 'comment.banner.cisco'
        'name': 'keyword.other.service'
        'patterns': [
          {
            'match': '^.*$'
            'name': 'comment.banner.cisco'
          }
        ]
      }
    ]
  'buffers':
    'patterns': [
